{
  "name": "probable",
  "version": "1.0.0",
  "description": "Probable - AI-powered prediction markets trading platform",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "agents:dev": "pnpm --filter @probable/agents dev",
    "detector:dev": "pnpm --filter @probable/trade-detector dev"
  },
  "keywords": [
    "polymarket",
    "prediction-markets",
    "trading",
    "agent",
    "claude",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "turbo": "^2.5.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
